{
  "name": "pebble-api",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Pebble.us",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "dev": "export NODE_ENV=development; ./node_modules/nodemon/bin/nodemon.js --exec ts-node src/api/index.ts --watch src",
    "test": "export NODE_ENV=testing; ./node_modules/jest/bin/jest.js --watch",
    "coverage": "jest --coverage",
    "db:rollback": "npm run build && npx sequelize-cli db:migrate:undo:all --options-path ./.sequelizerc-dist",
    "db:migrate": "npm run build && npx sequelize-cli db:migrate --options-path ./.sequelizerc-dist",
    "db:seed": "npm run build && npx sequelize-cli db:seed:all --options-path ./.sequelizerc-dist",
    "db:reset": "npm run db:rollback && npm run db:migrate && npm run db:seed"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.27",
    "@types/boom": "^7.3.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^25.1.4",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.136",
    "@types/node": "^13.9.5",
    "@types/validator": "^12.0.1",
    "jest": "^25.2.3",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/multer": "^1.4.7",
    "aes-js": "^3.1.2",
    "aws-sdk": "^2.1046.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "boom": "^7.3.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "joi": "^17.5.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "multer": "^1.4.4",
    "node-rsa": "^1.0.8",
    "nodemailer": "^6.7.2",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "promisify-es6": "^1.0.3",
    "randomstring": "^1.1.5",
    "sequelize": "^5.21.2",
    "twilio": "^3.71.3",
    "uuid": "^7.0.2",
    "winston": "^3.2.1",
    "yenv": "^2.1.0"
  }
}
